{
	"model_name":"struct_node",
	"LVT_aviliable":true,
	"Structure_aviliable":true,
	"Attention_aviliable":true,
	"Coverage_aviliable":false,
	"Parent_aviliable":false,
	"embedding":
	{
		"vocab":
		{
			"trainable":true,
			"initial":"glove",
			"dim":100,
			"dropout":0.0
		},
		"struct":
		{
			"trainable":true,
			"num":4,
			"_0":
			{
				"dim":16,
				"dropout":0.0
			},
			"_1":
			{
				"dim":16,
				"dropout":0.0
			},
			"_2":
			{
				"dim":16,
				"dropout":0.0
			},
			"_3":
			{
				"dim":16,
				"dropout":0.0
			}
		},
		"position":
		{
			"type":"both",
			"_abs":
			{
				"n_in":100,
				"n_out":16,
				"init_method":"clock",
				"threshold":10000
			},
			"_rel":
			{
				"calc_method":"embedding",
				"n_in":10,
				"n_out":16,
				"init_method":"clock",
				"threshold":10000
			},
			"dropout":0.0
		}
	},
	"encoder":
	{
		"type":"stacked",
		"n_layer":2,
		"encoder_0":
		{
			"type":"bi_lstm",
			"n_in":100,
			"n_out":256,
			"dropout":0.0
		},
		"encoder_1":
		{
			"type":"bi_lstm",
			"n_in":512,
			"n_out":256,
			"dropout":0.0
		}
	},
	"enc2dec":
	{
		"type":"feedforward",
		"method":"mean",
		"n_in":512,
		"n_out":256,
		"activation":"tanh"
	},
	"decoder":
	{
		"type":"struct_node_decoder",
		"_lstm":
		{
			"type":"lstm",
			"n_in":100,
			"n_out":256,
			"dropout":0.0
		},
		"_att_1":
		{
			"type":"attention",
			"n_e":512,
			"n_d":256,
			"n_att":256,
			"_softmax":
			{
				"type":"activation",
				"activation":"softmax"
			}
		},
		"_att_2":
		{
			"type":"attention",
			"n_e":196,
			"n_d":256,
			"n_att":256,
			"_softmax":
			{
				"type":"activation",
				"activation":"softmax"
			}
		},
		"_tanh":
		{
			"type":"feedforward",
			"n_in":768,
			"n_out":256,
			"activation":"tanh"
		},
		"_softmax":
		{
			"type":"feedforward",
			"n_in":256,
			"n_out":5000,
			"activation":"softmax"
		},
		"_switcher":
		{
			"type":"feedforward",
			"n_in":868,
			"n_out":1,
			"activation":"sigmoid"
		}
	}
}
